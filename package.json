{
  "name": "adherelive-be",
  "version": "3.2.0",
  "description": "All packages for the adhere.live nodejs backend application",
  "main": "constant.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adherelive/adherelive-web.git"
  },
  "keywords": [
    "adherelive",
    "web",
    "react",
    "mobile",
    "nodejs"
  ],
  "author": "gagneet.singh@adhere.live",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/adherelive/adherelive-web/issues"
  },
  "homepage": "https://github.com/adherelive/adherelive-web#readme",
  "scripts": {
    "start": "node --max-old-space-size=4096 ./node_modules/.bin/babel-node ./server/bin/www",
    "build": "npm run build --prefix client",
    "client": "npm run start --prefix client",
    "server": "node --max-old-space-size=4096 --exec ./node_modules/.bin/babel-node ./server/bin/www",
    "postinstall": "",
    "test": "jest",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "migrate": "./node_modules/.bin/babel-node ./node_modules/.bin/sequelize db:migrate",
    "seed": "./node_modules/.bin/babel-node ./node_modules/.bin/sequelize db:seed:all",
    "seed:feature": "./node_modules/.bin/babel-node ./node_modules/.bin/sequelize db:seed:all --seeders-path ./app/seeders/production",
    "undoSeed": "./node_modules/.bin/babel-node ./node_modules/.bin/sequelize db:seed:undo:all",
    "medicineDb": "./node_modules/.bin/babel-node ./scripts/use_pillbox.js",
    "degreeDb": "./node_modules/.bin/babel-node ./scripts/degreeDb",
    "rollback": "./node_modules/.bin/babel-node ./scripts/dropTables.js",
    "sqsCheck": "./node_modules/.bin/babel-node ./events/index.js",
    "prepare": "husky"
  },
  "nodemonConfig": {
    "watch": [
      "app/*",
      "server/*",
      "routes/*",
      "helper/*",
      "libs/*",
      "migrations/*"
    ],
    "delay": 2500
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,less,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.0",
    "@babel/node": "^7.26.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-arrow-functions": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "css-loader": "^7.1.2",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "less": "^4.2.1",
    "less-loader": "^12.2.0",
    "lint-staged": "^15.3.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "react-intl-translations-manager": "^5.0.3",
    "style-loader": "^4.0.0"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@faker-js/faker": "^9.3.0",
    "agora-access-token": "^2.0.4",
    "algoliasearch": "^5.18.0",
    "atob": "^2.1.2",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.7.9",
    "base64-img": "^1.0.4",
    "bcrypt": "^5.1.1",
    "chalk": "^5.4.1",
    "concurrently": "^9.1.2",
    "cookie-parser": "^1.4.7",
    "cookie-session": "^2.1.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "db-migrate": "^0.11.14",
    "db-migrate-mysql": "^3.0.0",
    "debug": "^4.4.0",
    "dotenv": "^16.4.7",
    "ejs": "^3.1.10",
    "exceljs": "^4.4.0",
    "express": "^4.21.2",
    "express-validator": "^7.2.1",
    "getstream": "^8.4.1",
    "google-auth-library": "^9.15.0",
    "got": "^14.4.5",
    "handlebars": "^4.7.8",
    "joi": "^17.13.3",
    "js-base64": "^3.7.7",
    "js-md5": "^0.8.3",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "minio": "^8.0.3",
    "moment": "^2.30.1",
    "mongodb": "^6.12.0",
    "mongoose": "^8.9.3",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.12.0",
    "node-fetch": "^3.3.2",
    "node-schedule": "^2.1.1",
    "node-xlsx": "^0.24.0",
    "nodemailer": "^6.9.16",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "papaparse": "^5.4.1",
    "pdfkit": "^0.16.0",
    "puppeteer": "^23.11.1",
    "razorpay": "^2.9.5",
    "react-chartjs-2": "^5.3.0",
    "react-markdown": "^9.0.1",
    "rrule": "^2.8.1",
    "sequelize": "^6.37.5",
    "sequelize-cli": "^6.6.2",
    "twilio": "^5.4.0",
    "uuid": "^11.0.3"
  }
}
