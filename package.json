{
  "name": "adherelive-be",
  "version": "3.0.0",
  "description": "All packages for the adhere.live web backend application",
  "main": "constant.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adherelive/adherelive-web.git"
  },
  "keywords": [
    "adherelive",
    "web",
    "react",
    "mobile",
    "android",
    "ios"
  ],
  "author": "gagneet.singh@adhere.live",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/adherelive/adherelive-web/issues"
  },
  "homepage": "https://github.com/adherelive/adherelive-web#readme",
  "scripts": {
    "start": "node --max-old-space-size=4096 ./node_modules/.bin/babel-node ./server/bin/www",
    "build": "npm run build --prefix client",
    "client": "npm run start --prefix client",
    "server": "node --max-old-space-size=4096 --exec ./node_modules/.bin/babel-node ./server/bin/www",
    "postinstall": "",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "migrate": "./node_modules/.bin/babel-node ./node_modules/.bin/sequelize db:migrate",
    "seed": "./node_modules/.bin/babel-node ./node_modules/.bin/sequelize db:seed:all",
    "seed:feature": "./node_modules/.bin/babel-node ./node_modules/.bin/sequelize db:seed:all --seeders-path ./app/seeders/production",
    "undoSeed": "./node_modules/.bin/babel-node ./node_modules/.bin/sequelize db:seed:undo:all",
    "medicineDb": "./node_modules/.bin/babel-node ./scripts/use_pillbox.js",
    "degreeDb": "./node_modules/.bin/babel-node ./scripts/degreeDb",
    "rollback": "./node_modules/.bin/babel-node ./scripts/dropTables.js",
    "sqsCheck": "./node_modules/.bin/babel-node ./events/index.js"
  },
  "nodemonConfig": {
    "watch": [
      "app/*",
      "server/*",
      "routes/*",
      "helper/*",
      "libs/*"
    ],
    "delay": 2500
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,less,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/node": "^7.18.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-arrow-functions": "^7.8.3",
    "@babel/preset-env": "^7.18.10",
    "css-loader": "^3.4.2",
    "husky": "^8.0.3",
    "jest": "^27.5.1",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "lint-staged": "^13.2.1",
    "nodemon": "^2.0.2",
    "prettier": "^2.4.1",
    "react-intl-translations-manager": "^5.0.3",
    "style-loader": "^1.1.3"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "agora-access-token": "^2.0.4",
    "algoliasearch": "^4.10.5",
    "atob": "^2.1.2",
    "aws-sdk": "^2.706.0",
    "axios": "^0.22.0",
    "base64-img": "^1.0.4",
    "bcrypt": "^5.0.0",
    "chalk": "^4.1.0",
    "concurrently": "^5.3.0",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.0.0",
    "date-fns": "^2.28.0",
    "db-migrate": "^0.11.13",
    "db-migrate-mysql": "^2.2.0",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.3",
    "express": "^4.17.3",
    "express-validator": "^6.6.0",
    "faker": "^5.5.3",
    "getstream": "5.0.5",
    "google-auth-library": "^7.10.0",
    "joi": "^17.4.2",
    "js-base64": "^3.7.2",
    "js-md5": "^0.7.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "minio": "^7.0.26",
    "moment": "^2.29.1",
    "mongoose": "^6.5.1",
    "multer": "^1.4.4",
    "mysql2": "^2.3.0",
    "node-fetch": "^2.6.1",
    "node-schedule": "^1.3.2",
    "node-xlsx": "^0.15.0",
    "xlsx": "^0.18.0",
    "nodemailer": "^6.4.10",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "papaparse": "^5.2.0",
    "pdfkit": "^0.13.0",
    "razorpay": "^2.0.6",
    "react-chartjs-2": "^2.9.0",
    "react-markdown": "^5.0.3",
    "request": "^2.88.2",
    "rrule": "2.6.8",
    "sequelize": "^5.21.8",
    "sequelize-cli": "^6.3.0",
    "twilio": "3.68.0",
    "uuid": "^8.3.2"
  }
}
