{
  "name": "adherelive-be",
  "version": "3.1.7",
  "description": "All packages for the adhere.live nodejs backend application",
  "main": "constant.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adherelive/adherelive-web.git"
  },
  "keywords": [
    "adherelive",
    "web",
    "react",
    "mobile",
    "nodejs"
  ],
  "author": "gagneet.singh@adhere.live",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/adherelive/adherelive-web/issues"
  },
  "homepage": "https://github.com/adherelive/adherelive-web#readme",
  "scripts": {
    "start": "node --trace-deprecation --max-old-space-size=4096 ./node_modules/.bin/babel-node ./server/bin/www",
    "server": "node --max-old-space-size=4096 --exec ./node_modules/.bin/babel-node ./server/bin/www",
    "postinstall": "",
    "migrate": "./node_modules/.bin/babel-node ./node_modules/.bin/sequelize db:migrate",
    "seed": "./node_modules/.bin/babel-node ./node_modules/.bin/sequelize db:seed:all",
    "seed:feature": "./node_modules/.bin/babel-node ./node_modules/.bin/sequelize db:seed:all --seeders-path ./app/seeders/production",
    "undoSeed": "./node_modules/.bin/babel-node ./node_modules/.bin/sequelize db:seed:undo:all",
    "medicineDb": "./node_modules/.bin/babel-node ./scripts/use_pillbox.js",
    "degreeDb": "./node_modules/.bin/babel-node ./scripts/degreeDb",
    "rollback": "./node_modules/.bin/babel-node ./scripts/dropTables.js",
    "sqsCheck": "./node_modules/.bin/babel-node ./events/index.js",
    "swagger-autogen": "node ./api-defn/swagger-docs.js",
    "dev": "npm run swagger-autogen && nodemon app.js"
  },
  "nodemonConfig": {
    "watch": [
      "app/*",
      "server/*",
      "routes/*",
      "helper/*",
      "libs/*",
      "migrations/*"
    ],
    "delay": 2500
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,less,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=16.10.0",
    "npm": ">=6.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.0",
    "@babel/node": "^7.26.0",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-arrow-functions": "^7.8.3",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "css-loader": "^3.4.2",
    "husky": "^4.2.3",
    "jest": "^27.5.1",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "lint-staged": "^10.0.7",
    "nodemon": "^2.0.2",
    "prettier": "^2.4.1",
    "react-intl-translations-manager": "^5.0.3",
    "style-loader": "^1.1.3"
  },
  "dependencies": {
    "@faker-js/faker": "^9.3.0",
    "@google-cloud/translate": "^8.5.0",
    "@hapi/joi": "^17.1.1",
    "agora-access-token": "^2.0.4",
    "algoliasearch": "^4.10.5",
    "atob": "^2.1.2",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.7.9",
    "base64-img": "^1.0.4",
    "bcrypt": "^5.0.0",
    "bull": "^4.16.5",
    "chalk": "^4.1.0",
    "concurrently": "^5.3.0",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.0.0",
    "date-fns": "^2.28.0",
    "db-migrate": "^0.11.13",
    "db-migrate-mysql": "^3.0.0",
    "debug": "^4.4.0",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.3",
    "exceljs": "^4.4.0",
    "express": "^4.17.3",
    "express-validator": "^6.6.0",
    "fs-extra": "^11.3.0",
    "getstream": "^5.0.5",
    "google-auth-library": "^7.10.0",
    "got": "^14.4.5",
    "handlebars": "^4.7.7",
    "ioredis": "^5.5.0",
    "joi": "^17.4.2",
    "js-base64": "^3.7.2",
    "js-md5": "^0.7.3",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "minio": "^7.0.26",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.47",
    "mongodb": "^6.12.0",
    "mongoose": "^8.9.5",
    "multer": "^1.4.4",
    "mysql2": "^3.9.8",
    "node-cache": "^5.1.2",
    "node-fetch": "^2.6.1",
    "node-schedule": "^1.3.2",
    "nodemailer": "^6.4.10",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "papaparse": "^5.2.0",
    "pdfkit": "^0.13.0",
    "puppeteer": "^14.2.0",
    "razorpay": "^2.0.6",
    "react-chartjs-2": "^2.9.0",
    "react-markdown": "^5.0.3",
    "redis": "^4.7.0",
    "rrule": "2.6.8",
    "sequelize": "^6.29.0",
    "sequelize-cli": "^6.3.0",
    "swagger-autogen": "^2.23.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "twilio": "3.68.0",
    "uuid": "^8.3.2",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  }
}
